{
    "properties": {        
        "amdPackages": "apprt@^3.2.1"
    },
    "load": {
        "allowedBundles": [
            "system@^3.2.1",
            "templatelayout@^3.2.1",
            "map@^3.2.1",
            "notifier@^3.2.1",
            "toolset@^3.2.1",
            "toolrules@^3.2.1",
            "parametermanager@^3.2.1",
            "clock"
        ]
    },
    "bundles": {
        "map": {
            "MapModelFactory": {
                "_configData": {
                    "maps": [
                        {
                            "baseLayer": [
                                {
                                    "id": "streets",
                                    "title": "Streets",
                                    "service": "streets",
                                    "layers": [
                                        "*"
                                    ]
                                }
                            ]
                        }
                    ]
                }
            },
            "MapState": {
                "initialExtent": {
                    "xmin": 600000,
                    "ymin": 5800000,
                    "xmax": 1700000,
                    "ymax": 7600000,
                    "spatialReference": {
                        "wkid": 3857
                    }
                }
            },
            "MappingResourceRegistryFactory": {
                "_knownServices": {
                    "services": [
                        {
                            "id": "streets",
                            "type": "AGS_TILED",
                            "url": "http://services.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer"
                        }
                    ]
                }
            }
        },
        "toolrules": {
            "ToolActiveStateManager": {
                "activateOnStartToolIds": [
                    "clockToggleTool"
                ]
            }
        },
        "toolset": {
            "ToolsetManager": {
                "toolsets": []
            }
        }
    }
}